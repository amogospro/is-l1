          <JSR223PostProcessor guiclass="TestBeanGUI" testclass="JSR223PostProcessor" testname="Extract Product ID">
            <stringProp name="cacheKey">true</stringProp>
            <stringProp name="filename"></stringProp>
            <stringProp name="parameters"></stringProp>
            <stringProp name="script">import groovy.json.JsonSlurper

// Get the response data
def response = prev.getResponseDataAsString()

// Parse JSON response
def jsonSlurper = new JsonSlurper()
def products = jsonSlurper.parseText(response)

// Get the current thread number (starts from 0, so add 1 if needed)
def threadNum = ctx.getThreadNum() + 1

// Construct the target username
def targetName = &quot;Product_AMOGUS&quot;

// Initialize user ID variable
def productId = null

// Iterate through the users array
for (product in products) {
    if (product.name == targetName) {
        productId = product.id
        break
    }
}

if (productId != null) {
    // Store user ID in a JMeter variable
    vars.put(&quot;product_id&quot;, productId.toString())
    log.info(&quot;Found product: ${targetName} with ID: ${productId}&quot;)
} else {
    // Handle the case when user is not found
    log.warn(&quot;Product ${targetName} not found in pending requests.&quot;)
    // Optionally set a variable to indicate failure
    vars.put(&quot;product_id&quot;, &quot;NOT_FOUND&quot;)
}
</stringProp>
