          <JSR223Assertion guiclass="TestBeanGUI" testclass="JSR223Assertion" testname="JSR223 Assertion" enabled="false">
            <stringProp name="cacheKey">true</stringProp>
            <stringProp name="filename"></stringProp>
            <stringProp name="parameters"></stringProp>
            <stringProp name="script">import java.util.concurrent.atomic.AtomicInteger

// Initialize AtomicInteger in JMeter properties
def deleteCounter = props.get(&quot;deleteCounter&quot;)
if (deleteCounter == null) {
    deleteCounter = new AtomicInteger(0)
    props.put(&quot;deleteCounter&quot;, deleteCounter)
}

// Increment the counter atomically
int currentCount = deleteCounter.incrementAndGet()

// Get the response code
String responseCode = prev.getResponseCode()

// Get the AssertionResult object
def AssertionResult = new AssertionResult()

// Logic for assertion
if (currentCount == 1) {
    // First DELETE request
    if (!(responseCode == &quot;200&quot; || responseCode == &quot;204&quot;)) {
        AssertionResult.setFailure(true)
        AssertionResult.setFailureMessage(&quot;First DELETE request did not receive 200 or 204. Received: &quot; + responseCode)
        Failure = true
        FailureMessage = AssertionResult.getFailureMessage()
    }
} else {
    // Subsequent DELETE requests
    if (responseCode != &quot;404&quot;) {
        AssertionResult.setFailure(true)
        AssertionResult.setFailureMessage(&quot;Subsequent DELETE request did not receive 404. Received: &quot; + responseCode)
        Failure = true
        FailureMessage = AssertionResult.getFailureMessage()
    }
}
</stringProp>
